{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.14.14",
    "@mui/lab": "^5.0.0-alpha.149",
    "@mui/material": "^5.11.9",
    "@mui/styles": "^5.14.14",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "axios": "^1.2.3",
    "date-fns": "^2.30.0",
    "deepcopy": "^2.1.0",
    "mapbox-gl": "^2.12.0",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-rnd": "^10.4.1",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4",
    "worker-loader": "^3.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build --force",
    "test": "react-scripts test",
    "test:coverage": "yarn run test --coverage",
    "test:report": "yarn run test -- --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts --ext .tsx",
    "lint:fix": "eslint . --ext .ts --ext .tsx --fix",
    "lint:report": "yarn run lint -- -f json -o eslint-report.json",
    "format": "prettier --check src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "format:fix": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json} --config ./.prettierrc",
    "stylelint": "stylelint src/**/*.css",
    "stylelint:fix": "stylelint src/**/*.css --fix",
    "stylelint:report": "yarn run stylelint -- -f json -o stylelint-report.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not safari < 10",
      "not chrome < 51",
      "not android < 5",
      "not ie < 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/enzyme": "^3.10.14",
    "@types/enzyme-adapter-react-16": "^1.0.7",
    "@types/mapbox-gl": "^2.7.10",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-sonarjs": "^0.21.0",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2"
  }
}
